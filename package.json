{
  "name": "david-site",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx @tailwindcss/cli -i ./static/src/styles.css -o ./static/dist/styles.css --watch",
    "build": "npx @tailwindcss/cli -i ./static/src/styles.css -o ./static/dist/styles.css --minify",
    "build-dev": "npx @tailwindcss/cli -i ./static/src/styles.css -o ./static/dist/styles.css",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "format-html": "prettier --write \"templates/**/*.html\""
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14"
  },
  "description": "tailwindcss -i ./static/src/styles.css -o ./static/dist/styles.css --watch tailwindcss -i ./static/src/styles.css -o ./static/dist/styles.css --minify",
  "main": "postcss.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avanyankar/david-site.git"
  },
  "keywords": [],
